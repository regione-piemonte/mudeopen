{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  
  "test": "456",
  "TEST": "123",
  
  
  
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "mudeopen-common": {
      "projectType": "library",
      "root": "projects/mudeopen-common",
      "sourceRoot": "projects/mudeopen-common/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/mudeopen-common/tsconfig.lib.json",
            "project": "projects/mudeopen-common/ng-package.json"
          },
          "configurations": {
            "local-rp-01": {
              "tsConfig": "projects/mudeopen-common/tsconfig.lib.json"
            },
            "dev-rp-01": {
              "tsConfig": "projects/mudeopen-common/tsconfig.lib.json"
            },
            "test-rp-01": {
              "tsConfig": "projects/mudeopen-common/tsconfig.lib.json"
            },
            "prod-rp-01": {
              "tsConfig": "projects/mudeopen-common/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/mudeopen-common/src/test.ts",
            "tsConfig": "projects/mudeopen-common/tsconfig.spec.json",
            "karmaConfig": "projects/mudeopen-common/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/mudeopen-common/tsconfig.lib.json",
              "projects/mudeopen-common/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }},
  "defaultProject": "mude-common-library"
}
